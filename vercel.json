{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/s3/:path*",
      "destination": "https://fls-9f649510-70dd-40c4-a4b9-572dc5dbe23e.laravel.cloud/:path*"
    }
  ],
  "headers": [
    {
      "source": "/s3/:path*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/returns",
      "destination": "/pages/returns",
      "permanent": true
    },
    {
      "source": "/payment",
      "destination": "/pages/payment",
      "permanent": true
    }
  ]
}